sers/amykang/Desktop/capstone/bobo-capstone/client/src/firebase.js",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/LoginOrSignup/LoginOrSignup.jsx",["128"],"import React, {Component} from 'react'\nimport UserLoginPage from '../pages/UserLoginPage/UserLoginPage'\nimport SignUpPage from '../pages/SignUpPage/SignUpPage';\nimport {Redirect} from 'react-router-dom';\n\nclass LoginOrSignup extends Component {\n    state = {\n        signup: true,\n        login: false,\n    }\n\n    signupRouter = (e) => {\n        e.preventDefault();\n        this.setState({\n            signup: false,\n        })\n    }\n\n\n    render() {\n       \n        if(!this.state.signup) {\n            // return <SignUpPage signup={this.signup}/>\n            return <Redirect to=\"/signup\"/>\n        } else {\n            return <UserLoginPage typeHandler={this.props.typeHandler} signupRouter = {this.signupRouter} loginHandler={this.props.loginHandler} />\n        }\n            // <div>\n            //     {this.state.signup? <UserLoginPage typeHandler={this.props.typeHandler}signupRouter = {this.signupRouter} loginHandler={this.props.loginHandler} />: <SignUpPage signup={this.signup}/>}\n            // </div>\n        \n    }\n}\nexport default LoginOrSignup\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/HomePage/HomePage.jsx",["129","130","131","132","133","134"],"import React, {useState, useEffect} from 'react';\nimport {db, auth} from '../../../firebase';\nimport './HomePage.scss';\nimport Header from '../../Header/Header';\nimport Footer from '../../Footer/Footer';\nimport SearchBar from '../../SearchBar/SearchBar';\nimport ScrollHero from '../../ScrollHero/ScrollHero';\nimport StoreList from '../../StoreList/StoreList';\nimport {Route} from 'react-router-dom';\nimport QrCodePage from '../../pages/QrCodePage/QrCodePage';\n\nfunction HomePage()  {\n  const [stores, setStores] = useState(null);\n  const [searchStore, setSearchStore] = useState(null)\n\n  useEffect(()=> {\n    db.collection('stores')\n    .get()\n    .then(snapshot=> {\n      const stores = []\n      snapshot.forEach(doc=> {\n        const data = doc.data()\n        data.uid = doc.id\n        stores.push(data)\n      })\n      setStores(stores)\n    })\n    .catch(err=> console.log(err))\n  }, [searchStore])\n  \n\n    let searchHandler = (e) => {\n      e.preventDefault();\n      console.log(typeof e.target.search.value)\n\n      stores.map(store => {\n        if (store.name.toLowerCase() === e.target.search.value.toLowerCase()) {\n          setSearchStore(store);\n        } else if (e.target.search.value == \"\") {\n          setSearchStore(null);\n        }\n      })\n    }\n\n    return (\n      <div className=\"home\">\n          <SearchBar searchHandler={searchHandler}/>\n          <ScrollHero/>\n          <StoreList stores={stores} searchStore={searchStore}/>\n          <Footer type=\"home\"/>\n        </div>\n    )\n    \n}\n\nexport default HomePage\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/Footer/Footer.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/UserLoginPage/UserLoginPage.jsx",["135","136"],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/SignUpPage/SignUpPage.jsx",["137"],"import React, {Component} from 'react';\nimport './SignUpPage.scss';\nimport {auth, db} from '../../../firebase';\nimport {Redirect} from 'react-router-dom';\n\nclass SignUpPage extends Component {\n    state = {\n        error:\"\",\n        useruid: \"\",\n        image: null,\n    }\n    signup = (e)=>  {\n        e.preventDefault();\n        auth.createUserWithEmailAndPassword(e.target.username.value, e.target.password.value)\n        .then(user=> {\n            this.setState({\n                useruid: user.user.uid\n            })\n            var userRef = db.collection(\"usertype\").add({\n                name: e.target.name.value,\n                username: e.target.username.value,\n                location:e.target.location.value,\n                uid: user.user.uid,\n                stores: [],\n                type: \"collector\",\n            })\n        })\n        .catch(err=>{\n            this.setState({\n                error: err.message\n            })\n        })\n    }\n\n    // onImageChange = event => {\n    //     if (event.target.files && event.target.files[0]) {\n    //       let img = event.target.files[0];\n    //       this.setState({\n    //         image: URL.createObjectURL(img)\n    //       });\n    //     }\n    //   };\n\n    render() {\n        if (this.state.useruid) {\n            return <Redirect to=\"/login\"/>\n        }\n    \n        return (\n            <div>\n                <h1>Sign Up</h1>\n                <form onSubmit={this.signup} className=\"signupForm\">\n                    <div>\n                        <label htmlFor=\"name\">name</label>\n                        <input type=\"text\" id=\"name\" name=\"name\"/>\n                    </div>\n                    <div>\n                        <label htmlFor=\"username\">email</label>\n                        <input type=\"email\" id=\"username\" name=\"username\"/>\n                    </div>\n                    <div>\n                        <label htmlFor=\"password\">password</label>\n                        <input type=\"password\" id=\"password\" name=\"password\"/>\n                    </div>\n                    <div>\n                        <label htmlFor=\"location\">location</label>\n                        <select id=\"location\" name=\"location\">\n                            <option value=\"vancouver\">Vancouver</option>\n                            <option value=\"toronto\">Toronto</option>\n                            <option value=\"toronto\">Calgary</option>\n                        </select>\n                    </div>\n                    <button >Sign up</button>\n                </form>\n            </div>\n        )\n\n    }\n}\n\nexport default SignUpPage","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/SearchBar/SearchBar.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/ScrollHero/ScrollHero.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/StoreList/StoreList.jsx",["138"],"import React from 'react';\nimport './StoreList.scss';\nimport store1 from '../../assets/images/store1.jpg'\nimport StoreListItem from '../StoreListItem/StoreListItem';\nimport {v4 as uuid} from 'uuid';\n\nexport default function StoreList({stores, searchStore}) {\n\n    if (searchStore) {\n        return <StoreListItem key={uuid()} store={searchStore}/>\n    } else if (!stores){\n        return <h1>loading...</h1> \n    }\n    return (\n        <div className=\"storeList\">\n            {stores.map(store => {\n                // console.log(store)\n                return <StoreListItem key={uuid()} store={store}/>\n            })}\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/Header/Header.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/FeaturedCard/FeaturedCard.jsx",["139"],"import React from 'react';\nimport './FeaturedCard.scss';\nimport store1 from '../../assets/images/store1.jpg';\n\nexport default function FeaturedCard() {\n    return (\n        <div className=\"card\">\n            <div className=\"card__imgBox\">\n                <img className=\"card__img\"src={`${store1}`}/>\n            </div>\n            <p className=\"card__name\">Boba shop name</p>\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/StoreListItem/StoreListItem.jsx",["140","141"],"import React, {Component} from 'react';\nimport './StoreListItem.scss';\nimport store1 from '../../assets/images/store1.jpg'\nimport {db, auth}from '../../firebase';\nimport firebase from 'firebase';\n\nclass StoreListItem extends Component {\n    clickHandler = () => {\n        db.collection('usertype')\n        .get()\n        .then(snapshot=> {\n            snapshot.forEach(doc=> {\n                if (doc.data().uid == auth.currentUser.uid) {\n                    let storesArray = db.collection(\"usertype\").doc(`${doc.id}`)\n                    storesArray.update({\n                        stores: firebase.firestore.FieldValue.arrayUnion({ id: this.props.store.uid, points: 0, name: this.props.store.name, pointmax: this.props.store.pointmax})\n                    })\n                }\n            })\n        })\n        .catch(err=> console.log(err))\n    }\n\n    render() {\n        return (\n            <div className=\"item\">\n            <div className=\"item__imgBox\">\n                <img className=\"item__img\"src={`${store1}`}/>\n            </div>\n            <div className=\"item__info\">\n                <div>\n                    <p className=\"item__info__storename\">{this.props.store.name}</p>\n                    <p className=\"item__info__des\">{this.props.store.description}</p>\n    \n                </div>\n            <button onClick={this.clickHandler} id={this.props.store.uid} className=\"item__info__btn\">Start Collecting</button>\n            </div>\n        </div>\n        )\n\n    }\n    \n    \n\n}\n\n\nexport default StoreListItem","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/PointsPage/PointsPage.jsx",["142","143","144","145","146","147"],"import React, {useState, useEffect} from 'react';\nimport './PointsPage.scss';\nimport Footer from '../../Footer/Footer';\nimport PointCard from '../../PointCard/PointCard';\nimport {auth, db} from '../../../firebase';\nimport {v4 as uuid} from 'uuid';\n\nexport default function PointsPage() {\n    const [stores, setStores] = useState([]);\n    const [store, setStore] = useState(null);\n    const [userPoints, setUserPoints] = useState(null);\n    const [maxPoints, setMaxPoints] = useState(null);\n\n    useEffect(() => {\n        db.collection('usertype')\n        .get()\n        .then(snapshot=> {\n            snapshot.forEach(doc=> {\n                if (doc.data().uid == auth.currentUser.uid) {\n                    setStores(doc.data().stores)\n                }\n            })\n        })\n        .catch(err=> console.log(err))\n    }, [store])\n\n\n\n    console.log(stores)\n\n    return (\n        <div>\n            <div className=\"pointsPage\">\n                <h1>Your Points</h1>\n                {stores.map(store=> <PointCard store={store} key={uuid()}/>)}\n            </div>\n            <Footer/>\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/PointCard/PointCard.jsx",["148","149"],"import React from 'react';\nimport './PointCard.scss';\nimport yellowStar from '../../assets/icons/point-yellow.svg';\nimport whiteStar from '../../assets/icons/point-white.svg';\n\nfunction starCounter(points) {\n    let array = [];\n    for (let i = 0; i < points; i++) {\n        array.push(i)\n    }\n    return array;\n}\n\n\nexport default function PointCard(props) {\n    let {store} = props\n    let maxPoints = store.pointmax;\n    let userPoints = starCounter(store.points);\n    let restOfPoints = starCounter(maxPoints - userPoints);\n    console.log(userPoints)\n    console.log(restOfPoints)\n\n    return (\n        <div className=\"pointcard\">\n            <h1>{store.name}</h1>\n            {/* <p>{store.points}</p>\n            <p>{store.pointmax}</p> */}\n            <div>\n                {userPoints.map(point=> <img className=\"pointcard__star\" src={yellowStar}/>)}\n                {restOfPoints.map(point=> <img className=\"pointcard__star\" src={whiteStar}/>)}\n            </div>\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/ProfilePage/ProfilePage.jsx",["150","151","152","153","154"],"import React, {useState, useEffect} from 'react';\nimport './ProfilePage.scss';\nimport Footer from '../../Footer/Footer';\nimport profile from '../../../assets/icons/profile.svg';\nimport edit from '../../../assets/icons/edit.svg';\nimport {db, auth} from '../../../firebase';\nimport firebase from 'firebase';\nimport EditModal from '../../EditModal/EditModal';\n\nexport default function ProfilePage(props) {\n    const [userInfo, setUserInfo] = useState({});\n    const [storeCount, setStoreCount] = useState(0);\n    const [pageLoader, setPageLoad] = useState(false);\n    const [show, setShow] = useState(false);\n\n    useEffect(()=> {\n        db.collection('usertype')\n        .get()\n        .then(snapshot=> {\n            snapshot.forEach(doc=> {\n                if (doc.data().uid == auth.currentUser.uid) {\n                    setUserInfo(doc.data())\n                    console.log(doc.data().stores)\n                    setStoreCount(doc.data().stores.length)\n                }\n            })\n        })\n        .catch(err=> console.log(err))\n    }, [pageLoader])\n\n    let updateUserInfo = (e) => {\n        e.preventDefault();\n        // console.log(e.target.name.value)\n        // console.log(e.target.username.value)\n        // console.log(e.target.location.value)\n        db.collection('usertype')\n        .get()\n        .then(snapshot=> {\n            snapshot.forEach(doc=> {\n                if (doc.data().uid == auth.currentUser.uid) {\n                    let userInfo = db.collection(\"usertype\").doc(`${doc.id}`)\n                    userInfo.update({\n                        name: e.target.name.value,\n                        // username: e.target.username.value,\n                        location: e.target.location.value\n                    })\n                }\n                setPageLoad(!pageLoader);\n                hideEdit()\n            })\n        })\n        .catch(err=> console.log(err))\n    }\n\n    let hideEdit = () => {\n        setShow(false);\n    }\n\n    let showEdit = () => {\n        setShow(true)\n    }\n\n    let logout =()=> {\n        auth.signOut();\n        props.history.push('/')\n      }\n\n  \n\n    return (\n        <div className=\"profile\">\n            <div className=\"profile__header\">\n                <img className=\"profile__header__edit\"src={edit} onClick={showEdit}/>\n            </div>\n            <div className=\"profile__info\">\n                <img className=\"profile__info__img\"src={profile}/>\n                <p>{userInfo.name}</p>\n                <p className=\"store\">Collecting points from <span className=\"store-count\">{storeCount}</span> boba stores</p>\n                <p>location: <span className=\"store-count\">{userInfo.location}</span> </p>\n                <p>email: <span className=\"store-count\">{userInfo.username}</span> </p>\n            </div>\n            <button onClick={logout}>logout</button>\n            <EditModal show={show} userInfo={userInfo} updateUserInfo={updateUserInfo} hideEdit={hideEdit}/>\n            <Footer/>\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/EditModal/EditModal.jsx",["155"],"import React from 'react';\nimport './EditModal.scss';\nimport profile from '../../assets/icons/profile.svg';\n\nexport default function EditModal({show, userInfo, updateUserInfo, hideEdit}) {\n    const showHideClassName = show ? \"modal display-block\" : \"modal display-none\";\n    return (\n        <div className={showHideClassName}>\n            <section className=\"modal-main\">\n                <div>\n                    <img className=\"modal-main__profile\" src={profile}/>\n                    {/* <div className=\"modal-main__profile modal-main__profile--active\">+ Upload</div> */}\n                </div>\n                <form className=\"signupForm\" onSubmit={updateUserInfo}>\n                    <div>\n                        <label htmlFor=\"name\" >name</label>\n                        <input className=\"modal-main__input\" type=\"text\" id=\"name\" name=\"name\" placeholder={userInfo.name}/>\n                    </div>\n                    {/* <div>\n                        <label htmlFor=\"username\">email</label>\n                        <input className=\"modal-main__input\" type=\"email\" id=\"username\" name=\"username\" placeholder={userInfo.username}/>\n                    </div> */}\n                    <div>\n                        <label htmlFor=\"location\">location</label>\n                        <select id=\"location\" name=\"location\">\n                            <option value=\"vancouver\">Vancouver</option>\n                            <option value=\"toronto\">Toronto</option>\n                            <option value=\"toronto\">Calgary</option>\n                        </select>\n                    </div>\n                    <button>Save</button>\n                </form>\n                <button onClick={hideEdit}>Cancel</button>\n            </section>\n        </div>\n    )\n}\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/StoreSignupPage/StoreSignupPage.jsx",["156"],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/FooterStore/FooterStore.jsx",[],{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","replacedBy":"160"},{"ruleId":"161","severity":1,"message":"162","line":5,"column":8,"nodeType":"163","messageId":"164","endLine":5,"endColumn":22},{"ruleId":"161","severity":1,"message":"165","line":4,"column":8,"nodeType":"163","messageId":"164","endLine":4,"endColumn":16},{"ruleId":"161","severity":1,"message":"166","line":5,"column":8,"nodeType":"163","messageId":"164","endLine":5,"endColumn":21},{"ruleId":"161","severity":1,"message":"167","line":6,"column":8,"nodeType":"163","messageId":"164","endLine":6,"endColumn":18},{"ruleId":"161","severity":1,"message":"168","line":8,"column":9,"nodeType":"163","messageId":"164","endLine":8,"endColumn":14},{"ruleId":"169","severity":1,"message":"170","line"