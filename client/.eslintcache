        <BrowserRouter>\n          <Provider store={store}>\n            <Switch>\n                <Route path=\"/\" exact component={()=> <LandingPage/>}/>\n                <Route path=\"/login\" exact component={(routerProps)=><LoginOrHome {...routerProps}/>}/>\n                <Route path=\"/signup\" exact component={(routerProps)=><SignUpPage {...routerProps}/>}/>\n                <Route path=\"/signup/store\" component={(routerProps)=> <StoreSignupPage {...routerProps}/>}/>\n                <Route path=\"/home\" exact component={(routerProps)=><HomePage {...routerProps}/>}/>\n                <Route path=\"/home/store\" exact component={StoreHomePage}/>\n                <Route path=\"/points\" component={PointsPage}/>\n                <Route path=\"/user/profile\" exact component={(routerProps)=> <ProfilePage {...routerProps}/>}/>\n                <Route path=\"/store/profile\" exact component={(routerProps)=><StoreProfilePage {...routerProps}/>}/>\n                <Route path=\"/home/user/qr\" component={QrCodePage}/>\n                <Route path=\"/store/users/manage\" component={PointManagePage}/>\n            </Switch>\n          </Provider>\n        </BrowserRouter>\n\n        \n      </div>\n    );\n\n  }\n}\n\nexport default App;\n","/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/LoginOrHome/LoginOrHome.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/StoreHomePage/StoreHomePage.jsx",["150","151","152","153","154","155","156"],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/LandingPage/LandingPage.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/pages/QrCodePage/QrCodePage.jsx",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/firebase.js",[],"/Users/amykang/Desktop/capstone/bobo-capstone/client/src/components/LoginOrSignup/LoginOrSignup.jsx",["157"],"import React, {Component} from 'react'\nimport UserLoginPage from '../pages/UserLoginP